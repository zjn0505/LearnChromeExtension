{
	"manifest_version" : 2,
	"name" : "QR generator",
	"version" : "1.0",

	"description" : "Create QR from current URL",
	"icons" : {
		"16" : "image/icon_16.png",
		"24" : "image/icon_24.png",
		"32" : "image/icon_32.png",
		"128" : "image/icon_128.png"
	},

	"browser_action" : {
		"default_popup" : "popup.html"
	},
	"permissions" : [
		"activeTab"
	]
}
